{
	"name": "dot11-components",
	"version": "1.9.97",
	"description": "Components for 802 tools",
	"browser": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"files": [
		"dist/",
		"store/",
		"src/",
		"package.json",
		"README.md"
	],
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rjstacey/dot11-components.git"
	},
	"author": "Robert Stacey <rjstacey@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/rjstacey/dot11-components/issues"
	},
	"homepage": "https://github.com/rjstacey/dot11-components#readme",
	"dependencies": {
		"bootstrap-icons": "^1.11.1",
		"file-saver": "^2.0.5",
		"lodash.debounce": "^4.0.8",
		"prop-types": "^15.8.1",
		"react-draggable": "^4.4.6",
		"react-expanding-textarea": "^2.3.2",
		"react-modal": "^3.16.1",
		"react-virtualized-auto-sizer": "^1.0.7",
		"react-window": "^1.8.10",
		"reselect": "^4.1.2"
	},
	"peerDependencies": {
		"@reduxjs/toolkit": "^1.9.7",
		"react": "^18.2.0",
		"react-redux": "^8.0.1"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.3",
		"@reduxjs/toolkit": "^1.9.7",
		"@storybook/addon-actions": "^8.4.7",
		"@storybook/addon-essentials": "^8.4.7",
		"@storybook/addon-links": "^8.4.7",
		"@storybook/node-logger": "^8.4.7",
		"@storybook/preset-create-react-app": "^8.4.7",
		"@storybook/react": "^8.4.7",
		"@storybook/react-webpack5": "^8.4.7",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"@types/file-saver": "^2.0.5",
		"@types/lodash.debounce": "^4.0.7",
		"@types/react-dom": "^18.2.4",
		"@types/react-modal": "^3.13.1",
		"@types/react-virtualized-auto-sizer": "^1.0.1",
		"@types/react-window": "^1.8.5",
		"@types/redux-logger": "^3.0.9",
		"copyfiles": "^2.4.1",
		"eslint-plugin-storybook": "^0.11.2",
		"http-proxy-middleware": "^2.0.0",
		"lorem-ipsum": "^2.0.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-redux": "^8.1.3",
		"redux-logger": "^3.0.6",
		"rimraf": "^4.4.0",
		"storybook": "^8.4.7",
		"tsconfig-paths-webpack-plugin": "^4.0.0",
		"typescript": "^4.9.5"
	},
	"scripts": {
		"start": "storybook dev -p 6006",
		"clean": "rimraf dist/*",
		"copy-files": "copyfiles -u 1 src/**/*.css dist/",
		"build": "npm run clean && tsc -p ./tsconfig.json && npm run copy-files",
		"storybook": "storybook dev -p 6006 -s public",
		"build-storybook": "storybook build -s public"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
