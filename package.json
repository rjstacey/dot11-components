{
  "name": "dot11-components",
  "version": "1.9.0",
  "description": "Components for 802 tools",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjstacey/dot11-components.git"
  },
  "author": "Robert Stacey <rjstacey@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rjstacey/dot11-components/issues"
  },
  "homepage": "https://github.com/rjstacey/dot11-components#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/draft-js": "^0.11.10",
    "@types/react-modal": "^3.13.1",
    "@types/react-window": "^1.8.5",
    "bootstrap-icons": "^1.8.0",
    "draft-js": "^0.11.7",
    "file-saver": "^2.0.5",
    "react-draggable": "^4.4.4",
    "react-dropdown-select": "^4.8.0",
    "react-expanding-textarea": "^2.3.2",
    "react-modal": "^3.14.4",
    "react-resize-detector": "^7.0.0",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.6",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@emotion/styled": "^11.1.5",
    "@reduxjs/toolkit": "^1.5.0",
    "react": "^17.0.0 || ^18.2.0",
    "react-redux": "^7.2.2 || ^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.0",
    "@reduxjs/toolkit": "^1.8.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/file-saver": "^2.0.5",
    "eslint": "^8.35.0",
    "eslint-plugin-storybook": "^0.6.11",
    "http-proxy-middleware": "^2.0.0",
    "lorem-ipsum": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.2",
    "react-scripts": "^5.0.1",
    "redux-logger": "^3.0.6",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.75.0"
  },
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; start-storybook -p 6006 -s public --no-manager-cache",
    "clean": "rm -rf dist/*",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prebuild__": "rm -rf dist/*",
    "postbuild__": "cp package.json dist",
    "build__": "babel src --out-dir dist --verbose --extensions \".ts\",\".tsx\",\".js\" --ignore \"src/stories\",\"**/*stories.{js,tsx}\",\"src/*.js\",\".test.js\" --no-copy-ignored --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/cjs/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "files": [
    "./dist/**/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
