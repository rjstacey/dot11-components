{
  "name": "dot11-components",
  "version": "1.9.56",
  "description": "Components for 802 tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "files": [
    "dist/",
    "store/",
    "src/",
    "package.json",
    "README.md"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjstacey/dot11-components.git"
  },
  "author": "Robert Stacey <rjstacey@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rjstacey/dot11-components/issues"
  },
  "homepage": "https://github.com/rjstacey/dot11-components#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "@types/lodash.debounce": "^4.0.7",
    "bootstrap-icons": "^1.11.1",
    "draft-js": "^0.11.7",
    "file-saver": "^2.0.5",
    "lodash.debounce": "^4.0.8",
    "react-draggable": "^4.4.4",
    "react-dropdown-select": "^4.8.0",
    "react-expanding-textarea": "^2.3.2",
    "react-modal": "^3.14.4",
    "react-resize-detector": "^7.0.0",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.6",
    "reselect": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@reduxjs/toolkit": "^1.9.3",
    "react": "17.0.2 - 18",
    "react-redux": "^7.2.2 || ^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@reduxjs/toolkit": "^1.9.3",
    "@storybook/addon-actions": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/node-logger": "^7.4.6",
    "@storybook/preset-create-react-app": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-webpack5": "^7.4.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/draft-js": "^0.11.10",
    "@types/file-saver": "^2.0.5",
    "@types/react-dom": "^18.2.4",
    "@types/react-modal": "^3.13.1",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/redux-logger": "^3.0.9",
    "copyfiles": "^2.4.1",
    "eslint": "^8.35.0",
    "eslint-plugin-storybook": "^0.6.15",
    "http-proxy-middleware": "^2.0.0",
    "lorem-ipsum": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "react-scripts": "^5.0.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^4.4.0",
    "storybook": "^7.4.6",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.75.0"
  },
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; storybook dev -p 6006 -s public",
    "clean": "rimraf dist/*",
    "copy-files": "copyfiles -u 1 src/**/*.css dist/",
    "build": "npm run clean && tsc -p ./tsconfig.json && npm run copy-files",
    "prebuild__": "rm -rf dist/*",
    "postbuild__": "cp package.json dist",
    "build__": "babel src --out-dir dist --verbose --extensions \".ts\",\".tsx\",\".js\" --ignore \"src/stories\",\"**/*stories.{js,tsx}\",\"src/*.js\",\".test.js\" --no-copy-ignored --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
